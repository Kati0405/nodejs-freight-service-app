{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "operator-linebreak": [
            "error",
            "after",
            {
              "overrides": {
                ":": "before"
              }
            }
          ],
          "no-underscore-dangle":  ["error", { "allow": ["_id"] }],
          "ObjectExpression": { "multiline": true, "minProperties": 1 }
          
    }
}
